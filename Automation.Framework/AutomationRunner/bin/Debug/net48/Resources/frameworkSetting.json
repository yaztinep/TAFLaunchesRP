{
  "BrowserType": "chrome", // [chrome/firefox/ie/safari]Default Browser Chrome
  "BrowserVersion": "", // Blank
  "GridHubUrl": "", // default is local. Grid url format --https://localhost:4444/wd/hub
  "Region": "T2", // Region name should match with Testing Region Json file if provided.
  "StepScreenShot": "off", // [on/off] Default is off. To get screen shot for each step mention ON otherwise screen shot of failure will be added in extent report.
